{
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["plugin:react/recommended"],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/jsx-uses-react": "off",
        "react/prop-types": "off",
        "react/no-unescaped-entities": 0
    },
    "plugins": ["react"],
    "settings": {
    "react": {
      "pragma": "React",
      "version": "detect"
    }
  }
}
  